	<!-- Descripción -->
	<div class="row bg-secondary">
		<p class="h5 text-center shadow-lg rounded text-light">
			Registro de un nuevo usuario.
		</p>
	</div>
	<!-- Formulario de registro para maestros -->
	<div class="container shadow-lg p-3 mb-5 rounded bg-secondary">
		<div class="row p-3">
			<h3 class="h3 text-center shadow-lg rounded text-light">Ingrese sus datos</h3>
		</div>
		<form class="col-sm shadow-lg p-3 mb-5 rounded text-light">
			<div class="form-row">
				<div class="row">
					<div class="form-group col-md-6">
						<label class="form-row" for="nombres">Nombres:</label>
						<input class="form-control row" type="text" id="nombres" name="nombres" required>
					</div>
					<div class="form-group col-md-6">
						<label class="form-row" for="apellidos">Apellidos:</label>
						<input class="form-control row" type="text" id="apellidos" name="apellidos" required>
					</div>
				</div>
				<div class="row">
					<div class="form-group col-md-6">
						<label class="form-row" for="correo">Correo:</label>
						<input class="form-control row" type="email" id="correo" name="correo" required>
					</div>
					<div class="form-group col-md-6">
						<label class="form-row" for="correoC">Clave:</label>
						<input class="form-control row" type="password" id="clave" name="clave" required>
					</div>
				</div>
			</div>
			<button type = "button" class="btn btn-lg col font-weight-bolder text-light" style = "background-color:#1E8449 ;" id="crear" >Crear cuenta</button>
				
		</form>
		<!-- Importar script-->
		<script src="public/registroUsuario.js"></script>
	</div>
	
	
